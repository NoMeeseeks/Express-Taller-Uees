<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
            <th scope="col">Email</th>
            <th scope="col">Fecha de Registro</th>
        </tr>
    </thead>
    <tbody>
        <!-- Iterar sobre los suscriptores que pasaste desde el backend -->
        <% suscriptores.forEach((suscriptor, index)=> { %>
            <tr>
                <th scope="row">
                    <%= index + 1 %>
                </th> <!-- Mostrar el índice +1 como número de fila -->
                <td>
                    <%= suscriptor.name %>
                </td> <!-- Nombre del suscriptor -->
                <td>
                    <%= suscriptor.email %>
                </td> <!-- Email del suscriptor -->
                <td>
                    <%= suscriptor.fecha_registro ? suscriptor.fecha_registro.toLocaleDateString() : '' %>
                </td> <!-- Fecha de registro -->
            </tr>
            <% }) %>
    </tbody>
</table>